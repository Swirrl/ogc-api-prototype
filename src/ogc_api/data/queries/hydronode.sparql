PREFIX wn: <http://environment.data.gov.uk/ogc/def/>
PREFIX geo: <http://www.opengis.net/ont/geosparql#>

CONSTRUCT {
  ?feature_uri ?p ?o .
  ?geo ?gp ?go .
}
WHERE {
#  ?feature_uri is passed in by query

    ?feature_uri a wn:HydroNode ;
                 geo:hasGeometry ?geo ;
                 ?p ?o .
    ?geo ?gp ?go .

}